        <conceptList>
            <concept code="0" codeSystem="2.16.756.5.30.1.1.1.1.3.6.3.11.8" displayName="INDEPENDENCE" level="0" type="L">
                <desc language="en-US">
          No help, preparation or supervision is required during each occurrence
        </desc>
                <desc language="fr-CH">
          INDÉPENDANCE – Aucune aide, préparation ou supervision
        </desc>
            </concept>
            <concept code="1" codeSystem="2.16.756.5.30.1.1.1.1.3.6.3.11.8" displayName="INDEPENDENCE WITH AID FOR PREPARATION ONLY" level="0" type="L">
                <desc language="fr-CH">
          INDÉPENDANCE AVEC AIDE POUR LA PRÉPARATION SEULEMENT
        </desc>
            </concept>
            <concept code="2" codeSystem="2.16.756.5.30.1.1.1.1.3.6.3.11.8" displayName="SUPERVISION" level="0" type="L">
                <desc language="en-US">
          Monitoring or indications required
        </desc>
                <desc language="fr-CH">
          SUPERVISION – Surveillance ou indications requises
        </desc>
            </concept>
            <concept code="3" codeSystem="2.16.756.5.30.1.1.1.1.3.6.3.11.8" displayName="LIMITED HELP" level="0" type="L">
                <desc language="en-US">
          Help required on a few occasions
        </desc>
                <desc language="fr-CH">
          AIDE LIMITÉE – Aide requise à quelques occasions
        </desc>
            </concept>
            <concept code="4" codeSystem="2.16.756.5.30.1.1.1.1.3.6.3.11.8" displayName="CONSIDERABLE HELP" level="0" type="L">
                <desc language="en-US">
          Help required throughout the activity, but the person performs 50% or more of the activity alone
        </desc>
                <desc language="fr-CH">
          AIDE CONSIDÉRABLE – Aide requise tout au long de l'activité, mais la personne effectue 50% ou plus de l'activité seule
        </desc>
            </concept>
            <concept code="5" codeSystem="2.16.756.5.30.1.1.1.1.3.6.3.11.8" displayName="MAXIMUM ASSISTANCE" level="0" type="L">
                <desc language="en-US">
          Help required throughout the activity and the person performs less than 50% of the activity alone
        </desc>
                <desc language="fr-CH">
          AIDE MAXIMALE – Aide requise tout au long de l'activité et la personne effectue moins de 50 % de l'activité seule
        </desc>
            </concept>
            <concept code="6" codeSystem="2.16.756.5.30.1.1.1.1.3.6.3.11.8" displayName="TOTAL DEPENDENCE" level="0" type="L">
                <desc language="en-US">
          Activity done entirely by other persons during each occurrence
        </desc>
                <desc language="fr-CH">
          DÉPENDANCE TOTALE – Activité entièrement effectuée par d’autres personnes au cours de la période évaluée
        </desc>
            </concept>
            <exception code="NASK" codeSystem="2.16.840.1.113883.5.1008" displayName="not asked" level="0" type="L"/>
            <exception code="NAV" codeSystem="2.16.840.1.113883.5.1008" displayName="temporarily unavailable" level="0" type="L"/>
            <exception code="ASKU" codeSystem="2.16.840.1.113883.5.1008" displayName="asked but unknown" level="0" type="L"/>
            <exception code="UNK" codeSystem="2.16.840.1.113883.5.1008" displayName="unknown" level="0" type="L"/>
        </conceptList>
