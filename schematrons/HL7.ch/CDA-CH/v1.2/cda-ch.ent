	<!--
	********************************************************
	Header Templates
	********************************************************
	-->
	<rule context="*[cda:templateId/@root=&quot;2.16.756.5.30.1.1.1.1.1&quot; and cda:templateId/@extension=&quot;CDA-CH.Head.Empl&quot;]">
		<!-- Rule Documentation -->
		<xhtml:h3 lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">CDA-CH Header Templates</xhtml:h3>
		<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Die nachfolgenden Regeln beschreiben die Prüfungen zu den CDA-CH Header Templates für Arbeitgeber</xhtml:p>
		<xhtml:h3 lang="fr_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">CDA-CH Header Templates</xhtml:h3>
		<xhtml:p lang="fr_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Les règles suivantes décrivent les contrôles effectués sur les CDA-CH Header Templates pour l'employeur</xhtml:p>
		<!--
		<xhtml:h3 lang='it_ch'>totranslate: italiano</xhtml:h3>
		<xhtml:p lang='it_ch'>totranslate: italiano</xhtml:p>
		-->
		<assert id="entity_HL7-cda-ch-0101" test="self::cda:participant/@typeCode=&quot;IND&quot;">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">CDA-CH 'Arbeitgeber' müssen als 'participant' mit typeCode 'IND' im CDA Header deklariert werden</xhtml:p>
			<xhtml:p lang="fr_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">L''Employeur' CDA-CH doit être déclaré comme 'participant' avec le typeCode 'IND' dans le CDA Header</xhtml:p>
			<!--
			<xhtml:p lang='it_ch'>totranslate: italiano</xhtml:p>
			-->
		</assert>
		<assert id="entity_HL7-cda-ch-0102" test="cda:templateId/@root=&quot;1.3.6.1.4.1.19376.1.5.3.1.2.2&quot;">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">CDA-CH Arbeitgeber müssen mit der IHE PCC Specification 'Employer and School Contacts' (1.3.6.1.4.1.19376.1.5.3.1.2) deklariert werden</xhtml:p>
			<xhtml:p lang="fr_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">L''Employeur' CDA-CH doit être déclaré avec l'IHE PCC Specification 'Employer and School Contacts' (1.3.6.1.4.1.19376.1.5.3.1.2)</xhtml:p>
			<!--
			<xhtml:p lang='it_ch'>totranslate: italiano</xhtml:p>
			-->
		</assert>
	</rule>
</pattern>
<pattern>
	<rule context="*[cda:templateId/@root=&quot;2.16.756.5.30.1.1.1.1.1&quot; and cda:templateId/@extension=&quot;CDA-CH.Head.Insurance&quot;]">
		<!-- Rule Documentation -->
		<xhtml:h3 lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">CDA-CH Header Templates</xhtml:h3>
		<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Die nachfolgenden Regeln beschreiben die Prüfungen zu den CDA-CH Header Templates für Versicherungen</xhtml:p>
		<xhtml:h3 lang="fr_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">CDA-CH Header Templates</xhtml:h3>
		<xhtml:p lang="fr_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Les règles suivantes décrivent les contrôles effectués sur les CDA-CH Header Templates pour l'assurance</xhtml:p>
		<!--
		<xhtml:h3 lang='it_ch'>totranslate: italiano</xhtml:h3>
		<xhtml:p lang='it_ch'>totranslate: italiano</xhtml:p>
		-->
		<assert id="entity_HL7-cda-ch-0201" test="self::cda:participant/@typeCode=&quot;COV&quot;">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">CDA-CH 'Versicherungen' müssen als 'participant' mit typeCode 'COV' im CDA Header deklariert werden</xhtml:p>
			<xhtml:p lang="fr_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">'Assurances' CDA-CH doivent être déclaré comme 'participant' avec le typeCode 'COV' dans le CDA Header</xhtml:p>
			<!--
			<xhtml:p lang='it_ch'>totranslate: italiano</xhtml:p>
			-->
		</assert>
		<assert id="entity_HL7-cda-ch-0202" test="self::cda:participant">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">CDA-CH 'Versicherungen' müssen als 'participant' im CDA Header deklariert werden</xhtml:p>
			<xhtml:p lang="fr_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Les 'Assurances' CDA-CH doivent être déclarées comme 'participant' dans le CDA Header</xhtml:p>
			<!--
			<xhtml:p lang='it_ch'>totranslate: italiano</xhtml:p>
			-->
		</assert>
		<assert id="entity_HL7-cda-ch-0203" test="cda:associatedEntity[@classCode=&quot;COVPTY&quot;]/cda:scopingOrganization">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">CDA-CH 'Versicherungen' müssen mit einer 'associatedEntity/scopingOrganization' und dem associatedEntity Attribut classCode="COVPTY" deklariert werden</xhtml:p>
			<xhtml:p lang="fr_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Les 'Assurances' CDA-CH doivent contenir une 'associatedEntity/scopingOrganization' avec lattribut classCode="COVPTY" pour associatedEntity</xhtml:p>
		</assert>
		<assert id="entity_HL7-cda-ch-0204" test="cda:associatedEntity[@classCode=&quot;COVPTY&quot;]/cda:id[@root=&quot;1.3.88&quot;]">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">CDA-CH 'Versicherungen' müssen mit der GLN identifiziert werden</xhtml:p>
			<xhtml:p lang="fr_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Les 'Assurances' CDA-CH doivent êtres identifies avec le GLN</xhtml:p>
		</assert>
	</rule>
</pattern>
<pattern>
	<rule context="*[cda:templateId/@root=&quot;2.16.756.5.30.1.1.1.1.1&quot; and cda:templateId/@extension=&quot;CDA-CH.Head.InsuranceCard&quot;]">
