	<rule context="*[cda:templateId/@root=&quot;2.16.756.5.30.1.1.1.1.1&quot; and cda:templateId/@extension=&quot;CDA-CH.Body.Rem&quot;]">
		<!-- Rule Documentation -->
		<xhtml:h3 lang="de_ch">CDA-CH Bemerkungen</xhtml:h3>
		<xhtml:p lang="de_ch">Die nachfolgenden Regeln beschreiben die Prüfungen zu den CDA-CH Bemerkungen</xhtml:p>
		
		<xhtml:h3 lang="fr_ch">CDA-CH Remarques</xhtml:h3>
		<xhtml:p lang="fr_ch">Les règles suivantes décrivent les contrôles sur 'Remarques'</xhtml:p>
		
		<xhtml:h3 lang='it_ch'>CDA-CH Osservazioni</xhtml:h3>
		<xhtml:p lang='it_ch'>Le regole seguenti descrivono i controlli per le CDA-CH Osservazioni</xhtml:p>
		
		<xhtml:h3 lang="en">CDA-CH remarks</xhtml:h3>
		<xhtml:p lang='en'>The following rules describe the tests on the CDA-CH remarks</xhtml:p>

		<assert id="entity_HL7-cda-ch-0401" test="self::cda:section">
			<xhtml:p lang="de_ch">CDA-CH 'Bemerkungen' müssen als 'section' im CDA Body deklariert werden</xhtml:p>
			<xhtml:p lang="fr_ch">Les 'Remarques' CDA-CH doivent être déclarées comme 'section' dans le CDA Body</xhtml:p>
			<xhtml:p lang='it_ch'>Le CDA-CH 'Osservazioni' devono essere dichiarate come 'section' nel CDA Body</xhtml:p>
			<xhtml:p lang='en'>CDA-CH 'Remarks' must be declared as a 'Section' in the CDA Body</xhtml:p>
		</assert>
		<assert id="entity_HL7-cda-ch-0402" test="(cda:title=&quot;Bemerkungen&quot; and //cda:languageCode/@code=&quot;de-CH&quot;) or //cda:languageCode/@code!=&quot;de-CH&quot;">
			<xhtml:p lang="de_ch">CDA-CH 'Bemerkungen' in Deutsch müssen einen section title 'Bemerkungen' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Les 'Remarques' CDA-CH en allemand doivent contenir un section title 'Bemerkungen'</xhtml:p>
			<xhtml:p lang='it_ch'>Le CDA-CH 'Osservazioni' in tedesco devono contenere un section title 'Bemerkungen'</xhtml:p>
			<xhtml:p lang='en'>CDA-CH 'Remarks' in German must contain a 'Bemerkungen' section title</xhtml:p>
		</assert>
		<assert id="entity_HL7-cda-ch-0403" test="(cda:title=&quot;Remarques&quot; and //cda:languageCode/@code=&quot;fr-CH&quot;) or //cda:languageCode/@code!=&quot;fr-CH&quot;">
			<xhtml:p lang="de_ch">CDA-CH 'Bemerkungen' in Französisch müssen einen section title 'Remarques' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Les 'Remarques' CDA-CH en français doivent contenir un section title 'Remarques'</xhtml:p>
			<xhtml:p lang='it_ch'>Le CDA-CH 'Osservazioni' in francese devono contenere un section title 'Remarques'</xhtml:p>
			<xhtml:p lang='en'>CDA-CH 'Remarks' in French must contain a 'Remarques' section title</xhtml:p>
		</assert>
		<assert id="entity_HL7-cda-ch-0404" test="(cda:title=&quot;Osservazioni&quot; and //cda:languageCode/@code=&quot;it-CH&quot;) or //cda:languageCode/@code!=&quot;it-CH&quot;">
			<xhtml:p lang="de_ch">CDA-CH 'Bemerkungen' in Italienisch müssen einen section title 'Osservazioni' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Les 'Remarques' CDA-CH en italien doivent contenir un section title 'Osservazioni'</xhtml:p>
			<xhtml:p lang='it_ch'>Le CDA-CH 'Osservazioni' in italiano devono contenere un section title 'Osservazioni'</xhtml:p>
			<xhtml:p lang='en'>CDA-CH 'Remarks' in Italian must contain a 'Osservazioni' section title</xhtml:p>
		</assert>
		<assert id="entity_HL7-cda-ch-0404" test="(cda:title=&quot;Remarks&quot; and //cda:languageCode/@code=&quot;en&quot;) or //cda:languageCode/@code!=&quot;en&quot;">
			<xhtml:p lang="de_ch">CDA-CH 'Bemerkungen' in Englisch müssen einen section title 'Remarks' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Les 'Remarques' CDA-CH en anglais doivent contenir un section title 'Remarks'</xhtml:p>
			<xhtml:p lang='it_ch'>Le CDA-CH 'Osservazioni' in inglese devono contenere un section title 'Remarks'</xhtml:p>
			<xhtml:p lang='en'>CDA-CH 'Remarks' in English must contain a 'Remarks' section title</xhtml:p>
		</assert>
	</rule>
</pattern>
<pattern>
	<!--
	********************************************************
	Diagnosenliste
	********************************************************
	-->
	<rule context="*[cda:templateId/@root=&quot;2.16.756.5.30.1.1.1.1.1&quot; and cda:templateId/@extension=&quot;CDA-CH.Body.DiagList&quot;]">
		<!-- Rule Documentation -->
		<xhtml:h3 lang="de_ch">CDA-CH Diagnosenliste</xhtml:h3>
		<xhtml:p lang="de_ch">Die nachfolgenden Regeln beschreiben die Prüfungen zu den CDA-CH Diagnosenliste</xhtml:p>

		<xhtml:h3 lang="fr_ch">CDA-CH liste de diagnostics</xhtml:h3>
		<xhtml:p lang="fr_ch">Les règles suivantes décrivent les contrôles sur la 'liste de diagnostics'</xhtml:p>

		<xhtml:h3 lang='it_ch'>CDA-CH Lista diagnosi</xhtml:h3>
		<xhtml:p lang='it_ch'>Le regole seguenti descrivono i controlli per la CDA-CH Lista diagnosi</xhtml:p>

		<xhtml:h3 lang='en'>CDA-CH list of diagnoses</xhtml:h3>
		<xhtml:p lang='en'>The following rules describe the tests on the CDA-CH list of diagnoses</xhtml:p>

		<assert id="entity_HL7-cda-ch-0501" test="self::cda:section">
			<xhtml:p lang="de_ch">CDA-CH 'Diagnosenliste' muss als 'section' im CDA Body deklariert werden</xhtml:p>
			<xhtml:p lang="fr_ch">La 'liste de diagnostics' CDA-CH doit être déclarée comme 'section' dans le CDA Body</xhtml:p>
			<xhtml:p lang='it_ch'>La CDA-CH 'Lista diagnosi' deve essere dichiarata come 'section' nel CDA Body</xhtml:p>
			<xhtml:p lang='en'>CDA-CH 'List of diagnoses' must be declared as a 'Section' in the CDA Body</xhtml:p>
		</assert>
		<assert id="entity_HL7-cda-ch-0502" test="cda:text">
			<xhtml:p lang="de_ch">CDA-CH 'Diagnosenliste' muss einen narrativen Text enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">La 'liste de diagnostics' CDA-CH doit contenir un texte narratif</xhtml:p>
			<xhtml:p lang='it_ch'>La CDA-CH 'Lista diagnosi' deve contenere un testo narrativo</xhtml:p>
			<xhtml:p lang='en'>CDA-CH 'List of diagnoses' must contain a narrative text</xhtml:p>
		</assert>
		<assert id="entity_HL7-cda-ch-0503" test="(cda:title=&quot;Diagnosen&quot; and //cda:languageCode/@code=&quot;de-CH&quot;) or //cda:languageCode/@code!=&quot;de-CH&quot;">
			<xhtml:p lang="de_ch">CDA-CH 'Diagnosenliste' in Deutsch muss einen section title 'Diagnosen' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">La 'liste de diagnostics' CDA-CH en allemand doit contenir un section title 'Diagnosen'</xhtml:p>
			<xhtml:p lang='it_ch'>La CDA-CH 'Lista diagnosi' in tedesco deve contenere un section title 'Diagnosen'</xhtml:p>
			<xhtml:p lang='en'>CDA-CH 'List of diagnoses' in German must contain a 'Diagnosen' section title</xhtml:p>
		</assert>
		<assert id="entity_HL7-cda-ch-0504" test="(cda:title=&quot;Diagnostics&quot; and //cda:languageCode/@code=&quot;fr-CH&quot;) or //cda:languageCode/@code!=&quot;fr-CH&quot;">
			<xhtml:p lang="de_ch">CDA-CH 'Diagnosenliste' in Französisch muss einen section title 'Diagnostics' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">La 'liste de diagnostics' CDA-CH en français doit contenir un section title 'Diagnostics'</xhtml:p>
			<xhtml:p lang='it_ch'>La CDA-CH 'Lista diagnosi' in francese deve contenere un section title 'Diagnostics'</xhtml:p>
			<xhtml:p lang='en'>CDA-CH 'List of diagnoses' in French must contain a 'Diagnostics' section title</xhtml:p>
		</assert>
		<assert id="entity_HL7-cda-ch-0505" test="(cda:title=&quot;Diagnosi&quot; and //cda:languageCode/@code=&quot;it-CH&quot;) or //cda:languageCode/@code!=&quot;it-CH&quot;">
			<xhtml:p lang="de_ch">CDA-CH 'Diagnosenliste' in Italienisch muss einen section title 'Diagnosi' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">La 'liste de diagnostics' CDA-CH en italien doit contenir un section title 'Diagnosi'</xhtml:p>
			<xhtml:p lang='it_ch'>La CDA-CH 'Lista diagnosi' in italiano deve contenere un section title 'Diagnosi'</xhtml:p>
			<xhtml:p lang='en'>CDA-CH 'List of diagnoses' in Italian must contain a 'Diagnosi' section title</xhtml:p>
		</assert>
		<assert id="entity_HL7-cda-ch-0506" test="(cda:title=&quot;Diagnoses&quot; and //cda:languageCode/@code=&quot;en&quot;) or //cda:languageCode/@code!=&quot;en&quot;">
			<xhtml:p lang="de_ch">CDA-CH 'Diagnosenliste' in Englisch muss einen section title 'Diagnoses' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">La 'liste de diagnostics' CDA-CH en anglais doit contenir un section title 'Diagnoses'</xhtml:p>
			<xhtml:p lang='it_ch'>La CDA-CH 'Lista diagnosi' in inglese deve contenere un section title 'Diagnoses'</xhtml:p>
			<xhtml:p lang='en'>CDA-CH 'List of diagnoses' in English must contain a 'Diagnoses' section title</xhtml:p>
		</assert>
		<assert id="entity_HL7-cda-ch-0507" test="cda:code/@codeSystem=&quot;2.16.840.1.113883.6.1&quot;">
			<xhtml:p lang="de_ch">CDA-CH 'Diagnosenliste' muss eine Codierung der Bedeutung der Section enthalten (gültiger Wert aus der Wertetabelle '2.16.840.1.113883.6.1')</xhtml:p>
			<xhtml:p lang="fr_ch">La 'liste de diagnostics' CDA-CH doit contenir un codage de la signification de la section (valeur valable du tableau de valeurs '2.16.840.1.113883.6.1')</xhtml:p>
			<xhtml:p lang='it_ch'>La CDA-CH 'Lista diagnosi' deve contenere un codice del significato della section (valore valido della tabella di valori '2.16.840.1.113883.6.1')</xhtml:p>
			<xhtml:p lang='en'>CDA-CH 'List of diagnoses' must contain a coding of the significance of the section (valid value from the table of values '2.16.840.1.113883.6.1')</xhtml:p>
		</assert>
		<assert id="entity_HL7-cda-ch-0508" test="cda:code/@code=document(&quot;../../../../schematrons/HL7.ch/CDA-CH/v1.2/cda-ch-voc.xml&quot;)/systems/system[@codeSystemName=&quot;DiagnosisSectionCode&quot;]/code/@value">
			<xhtml:p lang="de_ch">CDA-CH 'Diagnosenliste' muss eine Codierung der Bedeutung der Section enthalten (gültiger Wert aus der Wertetabelle '2.16.840.1.113883.6.1')</xhtml:p>
			<xhtml:p lang="fr_ch">La 'liste de diagnostics' CDA-CH doit contenir un codage de la signification de la section (valeur valable du tableau de valeurs '2.16.840.1.113883.6.1')</xhtml:p>
			<xhtml:p lang='it_ch'>La CDA-CH 'Lista diagnosi' deve contenere un codice del significato della section (valore valido della tabella di valori '2.16.840.1.113883.6.1')</xhtml:p>
