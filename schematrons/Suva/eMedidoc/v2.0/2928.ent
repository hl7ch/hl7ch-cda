	<rule context="cda:ClinicalDocument">
		<!-- Rule Documentation -->
		<xhtml:h3 lang="de_ch">Suva Ärztlicher Zwischenbericht: CDA Header</xhtml:h3>
		<xhtml:p lang="de_ch">Die nachfolgenden Regeln beschreiben die CDA Header Prüfungen für den Ärztlicher Zwischenbericht</xhtml:p>
		<xhtml:h3 lang="fr_ch">Suva rapport médical intermédiaire: CDA Header</xhtml:h3>
		<xhtml:p lang="fr_ch">Les règles suivantes décrivent les contrôles 'CDA Header' pour le rapport médical intermédiaire</xhtml:p>
		<xhtml:h3 lang="it_ch">Suva rapporto medico intermedio: CDA Header</xhtml:h3>
		<xhtml:p lang="it_ch">Le regole seguenti descrivono i controlli 'CDA Header' per il rapporto medico intermedio</xhtml:p>
		<xhtml:h3 lang="en">Suva interim medical report CDA Header</xhtml:h3>
		<xhtml:p lang="en">The following rules describe the CDA Header tests on the interim medical report</xhtml:p>
		<!-- Formularnummer prüfen -->
		<assert id="suva-form-2928-0101" test="//cda:templateId/@extension=&quot;2928&quot;">
			<xhtml:p lang="de_ch">Die Formularnummer für Suva Ärztlicher Zwischenbericht muss lauten: 2928</xhtml:p>
			<xhtml:p lang="fr_ch">Le numéro de formulaire pour le rapport médical intermédiaire de la Suva doit être: 2928</xhtml:p>
			<xhtml:p lang="it_ch">Il numero form. del rapporto medico intermedio Suva deve essere: 2928</xhtml:p>
			<xhtml:p lang="en">The form number for the Suva interim medical report must read: 2928</xhtml:p>
		</assert>
		<!-- code prüfen -->
		<assert id="suva-form-2928-0102" test="//cda:code/@code=&quot;51848-0&quot;">
			<xhtml:p lang="de_ch">Der LOINC Code für Der Suva Ärztliche Zwischenbericht muss lauten: 51848-0 (Assessment note)</xhtml:p>
			<xhtml:p lang="fr_ch">Le code LOINC pour le rapport médical intermédiaire de la Suva doit être: 51848-0 (Assessment note)</xhtml:p>
			<xhtml:p lang="it_ch">Il codice LOINC del rapporto medico intermedio Suva deve essere: 51848-0 (Assessment note)</xhtml:p>
			<xhtml:p lang="en">The LOINC Code for the Suva interim medical report must read: 51848-0 (Assessment note)</xhtml:p>
		</assert>
		<!-- title prüfen -->
		<assert id="suva-form-2928-0103" test="(//cda:title=&quot;Ärztlicher Zwischenbericht&quot; and //cda:languageCode/@code=&quot;de-CH&quot;) or //cda:languageCode/@code!=&quot;de-CH&quot;">
			<xhtml:p lang="de_ch">Der deutsche Titel muss lauten 'Ärztlicher Zwischenbericht'</xhtml:p>
			<xhtml:p lang="fr_ch">Le titre allemand doit être 'Ärztlicher Zwischenbericht'</xhtml:p>
			<xhtml:p lang="it_ch">Il titolo tedesco deve essere 'Ärztlicher Zwischenbericht'</xhtml:p>
			<xhtml:p lang="en">The German title must read 'Ärztlicher Zwischenbericht'</xhtml:p>
		</assert>
		<assert id="suva-form-2928-0104" test="(//cda:title=&quot;Rapport médical intermédiaire&quot; and //cda:languageCode/@code=&quot;fr-CH&quot;) or //cda:languageCode/@code!=&quot;fr-CH&quot;">
			<xhtml:p lang="de_ch">Der französische Titel muss lauten 'Rapport médical intermédiaire'</xhtml:p>
			<xhtml:p lang="fr_ch">Le titre français doit être 'Rapport médical intermédiaire'</xhtml:p>
			<xhtml:p lang="it_ch">Il titolo francese deve essere 'Rapport médical intermédiaire'</xhtml:p>
			<xhtml:p lang="en">The French title must read 'Rapport médical intermédiaire'</xhtml:p>
		</assert>
		<assert id="suva-form-2928-0105" test="(//cda:title=&quot;Rapporto medico intermedio&quot; and //cda:languageCode/@code=&quot;it-CH&quot;) or //cda:languageCode/@code!=&quot;it-CH&quot;">
			<xhtml:p lang="de_ch">Der italienische Titel muss lauten 'Rapporto medico intermedio'</xhtml:p>
			<xhtml:p lang="fr_ch">Le titre italien doit être 'Rapporto medico intermedio'</xhtml:p>
			<xhtml:p lang="it_ch">Il titolo italiano deve essere 'Rapporto medico intermedio'</xhtml:p>
			<xhtml:p lang="en">The Italian title must read 'Rapporto medico intermedio'</xhtml:p>
		</assert>
		<assert id="suva-form-2928-0106" test="(//cda:title=&quot;Interim medical report&quot; and //cda:languageCode/@code=&quot;en&quot;) or //cda:languageCode/@code!=&quot;en&quot;">
			<xhtml:p lang="de_ch">Der englische Titel muss lauten 'Interim medical report'</xhtml:p>
			<xhtml:p lang="fr_ch">Le titre anglais doit être 'Interim medical report'</xhtml:p>
			<xhtml:p lang="it_ch">Il titolo inglese deve essere 'Interim medical report'</xhtml:p>
			<xhtml:p lang="en">The English title must read 'Interim medical report'</xhtml:p>
		</assert>
		<!-- Arbeitgeber prüfen -->
		<assert id="suva-form-2928-0107" test="//cda:participant/cda:templateId[@root=&quot;2.16.756.5.30.1.1.1.1.1&quot; and @extension=&quot;CDA-CH.Head.Empl&quot;]">
			<xhtml:p lang="de_ch">Der Suva Ärztliche Zwischenbericht muss einen Arbeitgeber enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical intermédiaire de la Suva doit contenir un employeur</xhtml:p>
			<xhtml:p lang="it_ch">Nel rapporto medico intermedio Suva deve essere indicato un datore di lavoro</xhtml:p>
			<xhtml:p lang="en">The Suva interim medical report must contain an employer</xhtml:p>
		</assert>
		<!--
		********************************************************
		Aufbau des Dokuments (Existenz der Sections prüfen)
		********************************************************
		-->
		<assert id="suva-form-2928-0108" test="//cda:templateId/@root=&quot;2.16.756.5.30.1.100.2.2.1&quot; and //cda:templateId/@extension=&quot;eMedidoc.SchadenInfo&quot;">
			<xhtml:p lang="de_ch">Der Suva Ärztliche Zwischenbericht muss eine Section 'Schadeninformationen' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical intermédiaire de la Suva doit contenir une section 'Informations sur le sinistre'</xhtml:p>
			<xhtml:p lang="it_ch">Il rapporto medico intermedio Suva deve contenere una sezione 'Informazioni sul sinistro'</xhtml:p>
			<xhtml:p lang="en">The Suva interim medical report must contain a 'Claim information' section</xhtml:p>
		</assert>
		<assert id="suva-form-2928-0109" test="//cda:templateId/@root=&quot;2.16.756.5.30.1.1.1.1.1&quot; and //cda:templateId/@extension=&quot;CDA-CH.Body.DiagList&quot;">
			<xhtml:p lang="de_ch">Der Suva Ärztliche Zwischenbericht muss eine Section 'Diagnosen' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical intermédiaire de la Suva doit contenir une section 'Diagnostics'</xhtml:p>
			<xhtml:p lang="it_ch">Il rapporto medico intermedio Suva deve contenere una sezione 'Diagnosi'</xhtml:p>
			<xhtml:p lang="en">The Suva interim medical report must contain a 'Diagnoses' section</xhtml:p>
		</assert>
		<assert id="suva-form-2928-0110" test="//cda:templateId/@root=&quot;2.16.756.5.30.1.100.2&quot; and //cda:templateId/@extension=&quot;Suva.2928.Verlauf&quot;">
			<xhtml:p lang="de_ch">Der Suva Ärztliche Zwischenbericht muss eine Section 'Verlauf' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical intermédiaire de la Suva doit contenir une section 'Evolution'</xhtml:p>
			<xhtml:p lang="it_ch">Il rapporto medico intermedio Suva deve contenere una sezione 'Decorso'</xhtml:p>
			<xhtml:p lang="en">The Suva interim medical report must contain a 'Course' section</xhtml:p>
		</assert>
		<assert id="suva-form-2928-0111" test="//cda:templateId/@root=&quot;2.16.756.5.30.1.100.2&quot; and //cda:templateId/@extension=&quot;Suva.2928.Therapie&quot;">
			<xhtml:p lang="de_ch">Der Suva Ärztliche Zwischenbericht muss eine Section 'Therapie' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical intermédiaire de la Suva doit contenir une section 'Thérapie'</xhtml:p>
			<xhtml:p lang="it_ch">Il rapporto medico intermedio Suva deve contenere una sezione 'Terapia'</xhtml:p>
			<xhtml:p lang="en">The Suva interim medical report must contain a 'Therapy' section</xhtml:p>
		</assert>
		<assert id="suva-form-2928-0112" test="//cda:templateId/@root=&quot;2.16.756.5.30.1.1.1.1.1&quot; and //cda:templateId/@extension=&quot;CDA-CH.Body.ArbeitsfähigkeitList&quot;">
			<xhtml:p lang="de_ch">Der Suva Ärztliche Zwischenbericht muss eine Section 'Arbeitsfähigkeit' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical intermédiaire de la Suva doit contenir une section 'Capacité de travail'</xhtml:p>
			<xhtml:p lang="it_ch">Il rapporto medico intermedio Suva deve contenere una sezione 'Capacità lavorativa'</xhtml:p>
			<xhtml:p lang="en">The Suva interim medical report must contain a 'Capacity to work' section</xhtml:p>
		</assert>
		<assert id="suva-form-2928-0113" role="information" test="//cda:templateId/@root=&quot;2.16.756.5.30.1.100.2.2.1&quot; and //cda:templateId/@extension=&quot;eMedidoc.BehAbschluss&quot;">
			<xhtml:p lang="de_ch">Der Suva Ärztliche Zwischenbericht enthält keine Section 'Behandlungsabschluss'</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical intermédiaire de la Suva ne contient pas de section 'Traitement terminé'</xhtml:p>
			<xhtml:p lang="it_ch">Il rapporto medico intermedio non contiene una sezione 'Conclusione del trattamento'</xhtml:p>
			<xhtml:p lang="en">The Suva interim medical report does not contain an 'End of treatment' section</xhtml:p>
		</assert>
		<assert id="suva-form-2928-0114" role="information" test="//cda:templateId/@root=&quot;2.16.756.5.30.1.100.2.2.1&quot; and //cda:templateId/@extension=&quot;Suva.2928.ZuweisungArbeit&quot;">
			<xhtml:p lang="de_ch">Der Suva Ärztliche Zwischenbericht enthält keine Section 'Behandlungsabschluss, Zuweisung Arbeit'</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical intermédiaire de la Suva ne contient pas de section 'Traitement terminé, Procurer un travail'</xhtml:p>
			<xhtml:p lang="it_ch">Il rapporto medico intermedio non contiene una sezione 'Conclusione del trattamento, assegnazione lavoro'</xhtml:p>
			<xhtml:p lang="en">The Suva interim medical report does not contain an 'End of treatment, allocation of work' section</xhtml:p>
		</assert>
		<assert id="suva-form-2928-0115" role="information" test="//cda:templateId/@root=&quot;2.16.756.5.30.1.100.2.2.1&quot; and //cda:templateId/@extension=&quot;Suva.2928.BleibendeNachteile&quot;">
			<xhtml:p lang="de_ch">Der Suva Ärztliche Zwischenbericht enthält keine Section 'Behandlungsabschluss, Zuweisung Arbeit'</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical intermédiaire de la Suva ne contient pas de section 'Traitement terminé, Procurer un travail'</xhtml:p>
			<xhtml:p lang="it_ch">Il rapporto medico intermedio non contiene una sezione 'Conclusione del trattamento, assegnazione lavoro'</xhtml:p>
			<xhtml:p lang="en">T The Suva interim medical report does not contain an 'End of treatment, allocation of work' section</xhtml:p>
		</assert>
		<assert id="suva-form-2928-0116" test="//cda:templateId/@root=&quot;2.16.756.5.30.1.1.1.1.1&quot; and //cda:templateId/@extension=&quot;eMedidoc.Rem&quot;">
			<xhtml:p lang="de_ch">Der Suva Ärztliche Zwischenbericht muss eine Section 'Bemerkungen' enthalten (mit Erwerbstatus. Wenn keine Bemerkungen, dann 'Keine' angeben)</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical intermédiaire de la Suva doit contenir une section 'Remarques' (même s'il n'y a pas de remarques – indiquer alors 'Aucune')</xhtml:p>
			<xhtml:p lang="it_ch">Il rapporto medico intermedio Suva deve contenere una sezione 'Osservazioni' (con situazione lavorativa. Se non ci sono osservazioni, scrivere 'Nessuna')</xhtml:p>
			<xhtml:p lang="en">The Suva interim medical report must contain a 'Remarks' section (with employment status. If no remarks, then enter 'None')</xhtml:p>
		</assert>
	</rule>
	<rule context="*[cda:templateId/@root=&quot;2.16.756.5.30.1.100.2&quot; and cda:templateId/@extension=&quot;Suva.2928.Verlauf&quot;]">
		<!-- Rule Documentation -->
		<xhtml:h3 lang="de_ch">Suva Ärztlicher Zwischenbericht: CDA Body Section 'Verlauf'</xhtml:h3>
		<xhtml:p lang="de_ch">Die nachfolgenden Regeln beschreiben die CDA Body Prüfungen für 'Verlauf'</xhtml:p>
		<xhtml:h3 lang="fr_ch">Suva rapport médical intermédiaire: CDA Body Section 'Evolution'</xhtml:h3>
		<xhtml:p lang="fr_ch">Les règles suivantes décrivent les contrôles CDA Body pour 'Evolution'</xhtml:p>
		<xhtml:h3 lang="it_ch">Suva rapporto medico intermedio: CDA Body Section 'Decorso'</xhtml:h3>
		<xhtml:p lang="it_ch">Le regole seguenti descrivono i controlli 'CDA Body' per 'Decorso'</xhtml:p>
		<xhtml:h3 lang="en">Suva interim medical report: CDA Body Section 'Course'</xhtml:h3>
		<xhtml:p lang="en">The following rules describe the CDA Body tests on 'Course'</xhtml:p>
		<assert id="suva-form-2928-0201" test="self::cda:section">
			<xhtml:p lang="de_ch">eMedidoc 'Verlauf' muss als 'section' im CDA Body deklariert werden</xhtml:p>
			<xhtml:p lang="fr_ch">'Evolution' eMedidoc doit être déclarée comme 'section' dans le CDA Body</xhtml:p>
			<xhtml:p lang="it_ch">'Decorso' eMedidoc deve essere dichiarato come 'section' nel CDA Body</xhtml:p>
			<xhtml:p lang="en">eMedidoc 'Course' must be declared as a 'Section' in the CDA Body</xhtml:p>
		</assert>
		<assert id="suva-form-2928-0202" test="cda:text">
			<xhtml:p lang="de_ch">eMedidoc 'Verlauf' muss einen narrativen Text enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">'Evolution' eMedidoc doit contenir un texte narratif</xhtml:p>
			<xhtml:p lang="it_ch">'Decorso' eMedidoc deve contenere un testo narrativo</xhtml:p>
			<xhtml:p lang="en">eMedidoc 'Course' must contain a narrative text</xhtml:p>
		</assert>
		<assert id="suva-form-2928-0203" test="(cda:title=&quot;Verlauf&quot; and //cda:languageCode/@code=&quot;de-CH&quot;) or //cda:languageCode/@code!=&quot;de-CH&quot;">
			<xhtml:p lang="de_ch">Deutsch: eMedidoc 'Verlauf' muss einen section title 'Verlauf' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Allemand: 'Evolution' eMedidoc doit contenir un section title 'Verlauf'</xhtml:p>
			<xhtml:p lang="it_ch">Tedesco: 'Decorso' eMedidoc deve contenere un section title 'Verlauf'</xhtml:p>
			<xhtml:p lang="en">German: eMedidoc 'Course' must contain a 'Verlauf' section title</xhtml:p>
		</assert>
		<assert id="suva-form-2928-0204" test="(cda:title=&quot;Evolution&quot; and //cda:languageCode/@code=&quot;fr-CH&quot;) or //cda:languageCode/@code!=&quot;fr-CH&quot;">
			<xhtml:p lang="de_ch">Französisch: eMedidoc 'Verlauf' muss einen section title 'Evolution' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Français: 'Evolution' eMedidoc doit contenir un section title 'Evolution'</xhtml:p>
			<xhtml:p lang="it_ch">Francese: 'Decorso' eMedidoc deve contenere un section title 'Evolution'</xhtml:p>
			<xhtml:p lang="en">French: eMedidoc 'Course' must contain a 'Evolution' section title</xhtml:p>
		</assert>
		<assert id="suva-form-2928-0205" test="(cda:title=&quot;Decorso&quot; and //cda:languageCode/@code=&quot;it-CH&quot;) or //cda:languageCode/@code!=&quot;it-CH&quot;">
			<xhtml:p lang="de_ch">Italienisch: eMedidoc 'Verlauf' muss einen section title 'Decorso' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Italien: 'Evolution' eMedidoc doit contenir un section title 'Decorso'</xhtml:p>
			<xhtml:p lang="it_ch">Italiano: 'Decorso' eMedidoc deve contenere un section title 'Decorso'</xhtml:p>
			<xhtml:p lang="en">Italian: eMedidoc 'Course' must contain a 'Decorso' section title</xhtml:p>
		</assert>
		<assert id="suva-form-2928-0206" test="(cda:title=&quot;Course&quot; and //cda:languageCode/@code=&quot;en&quot;) or //cda:languageCode/@code!=&quot;en&quot;">
			<xhtml:p lang="de_ch">Englisch: eMedidoc 'Verlauf' muss einen section title 'Course' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Anglais: 'Evolution' eMedidoc doit contenir un section title 'Course'</xhtml:p>
			<xhtml:p lang="it_ch">Inglese: 'Decorso' eMedidoc deve contenere un section title 'Course'</xhtml:p>
			<xhtml:p lang="en">English: eMedidoc 'Course' must contain a 'Course' section title</xhtml:p>
		</assert>
		<assert id="suva-form-2928-0207" test="cda:code/@code=&quot;47519-4&quot; and cda:code/@codeSystem=&quot;2.16.840.1.113883.6.1&quot;">
			<xhtml:p lang="de_ch">eMedidoc 'Verlauf' muss mit dem LOINC Code 47519-4 (History of Procedures) codiert werden</xhtml:p>
			<xhtml:p lang="fr_ch">'Evolution' eMedidoc doit être codée avec le code LOINC 47519-4 (History of Procedures)</xhtml:p>
			<xhtml:p lang="it_ch">'Decorso' eMedidoc deve essere codificato con il codice LOINC 47519-4 (History of Procedures)</xhtml:p>
			<xhtml:p lang="en">eMedidoc 'Course' must be coded with the LOINC Code 47519-4 (History of Procedures)</xhtml:p>
		</assert>
		<assert id="suva-form-2928-0208" test="descendant::*[cda:templateId/@root=&quot;2.16.756.5.30.1.100.2.2.1&quot; and cda:templateId/@extension=&quot;Suva.2928.BishVerlauf&quot;]">
			<xhtml:p lang="de_ch">eMedidoc 'Verlauf' muss eine min. 1 organizer 'Bisheriger Verlauf' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">'Evolution' eMedidoc doit contenir au moins 1 organizer 'Evolution précédente'</xhtml:p>
			<xhtml:p lang="it_ch">'Decorso' eMedidoc deve contenere almeno 1 organizer 'Decorso precedente'</xhtml:p>
			<xhtml:p lang="en">eMedidoc 'Course' must contain at least 1 'Previous course' organizer</xhtml:p>
		</assert>
	</rule>
	<rule context="*[cda:templateId/@root=&quot;2.16.756.5.30.1.100.2.2.1&quot; and cda:templateId/@extension=&quot;Suva.2928.BishVerlauf&quot;]">
		<!-- Rule Documentation -->
		<xhtml:h3 lang="de_ch">Suva Ärztlicher Zwischenbericht: CDA Body Section 'Verlauf - Organizer'</xhtml:h3>
		<xhtml:p lang="de_ch">Die nachfolgenden Regeln beschreiben die CDA Body Prüfungen für 'Verlauf - Organizer'</xhtml:p>
		<xhtml:h3 lang="fr_ch">Suva rapport médical intermédiaire: CDA Body Section 'Evolution - Organizer'</xhtml:h3>
		<xhtml:p lang="fr_ch">Les règles suivantes décrivent les contrôles CDA Body pour 'Evolution - Organizer'</xhtml:p>
		<xhtml:h3 lang="it_ch">Suva rapporto medico intermedio: CDA Body Section 'Decorso - Organizer'</xhtml:h3>
		<xhtml:p lang="it_ch">Le regole seguenti descrivono i controlli 'CDA Body' per 'Decorso - Organizer'</xhtml:p>
		<xhtml:h3 lang="en">Suva interim medical report: CDA Body Section 'Course - Organizer'</xhtml:h3>
		<xhtml:p lang="en">The following rules describe the CDA Body tests on 'Course - Organizer'</xhtml:p>
		<assert id="suva-form-2928-0301" test="self::cda:organizer">
			<xhtml:p lang="de_ch">eMedidoc 'Verlauf' muss als 'organizer' im CDA Body (section 'Verlauf') deklariert werden</xhtml:p>
			<xhtml:p lang="fr_ch">'Evolution' eMedidoc doit être déclarée comme 'organizer' dans le CDA Body (section 'Evolution')</xhtml:p>
			<xhtml:p lang="it_ch">'Decorso' eMedidoc deve essere dichiarato come 'organizer' nel CDA Body (section 'Decorso')</xhtml:p>
			<xhtml:p lang="en">eMedidoc 'Course' must be declared as an 'Organizer' in the CDA Body (section 'Course')</xhtml:p>
		</assert>
		<assert id="suva-form-2928-0302" test="cda:code/@code=&quot;47519-4&quot; and cda:code/@codeSystem=&quot;2.16.840.1.113883.6.1&quot;">
			<xhtml:p lang="de_ch">eMedidoc 'Verlauf' muss mit dem LOINC Code 47519-4 (History of Procedures) codiert werden</xhtml:p>
			<xhtml:p lang="fr_ch">'Evolution' eMedidoc doit être codée avec le code 47519-4 (History of Procedures)</xhtml:p>
			<xhtml:p lang="it_ch">'Decorso' eMedidoc deve essere codificato con il codice LOINC 47519-4 (History of Procedures)</xhtml:p>
			<xhtml:p lang="en">eMedidoc 'Course' must be coded with the LOINC Code 47519-4 (History of Procedures)</xhtml:p>
		</assert>
		<assert id="suva-form-2928-0303" test="cda:effectiveTime">
			<xhtml:p lang="de_ch">eMedidoc 'Verlauf' muss ein Beurteilungsdatum enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">'Evolution' eMedidoc doit contenir une date d'évaluation</xhtml:p>
			<xhtml:p lang="it_ch">'Decorso' eMedidoc deve contenere una data di valutazione</xhtml:p>
			<xhtml:p lang="en">eMedidoc 'Course' must contain an assessment date</xhtml:p>
		</assert>
		<assert id="suva-form-2928-0304" test="cda:performer/cda:assignedEntity/cda:assignedPerson/cda:name">
