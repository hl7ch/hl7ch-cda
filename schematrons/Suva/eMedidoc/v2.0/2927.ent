<pattern>
	<!-- Entity Information -->
	<xhtml:ul id="suva-form-2927">
		<xhtml:li class="filename">2927.ent</xhtml:li>
		<xhtml:li class="version">1.0</xhtml:li>
	</xhtml:ul>
	<rule context="cda:ClinicalDocument">
		<!-- Rule Documentation -->
		<xhtml:h3 lang="de_ch">Suva Arztzeugnis UVG: CDA Header</xhtml:h3>
		<xhtml:p lang="de_ch">Die nachfolgenden Regeln beschreiben die CDA Header Prüfungen für das Suva Arztzeugnis</xhtml:p>
		<xhtml:h3 lang="fr_ch">Suva certificat de médecin UVG: CDA Header</xhtml:h3>
		<xhtml:p lang="fr_ch">Les règles suivantes décrivent les contrôles 'CDA Header' pour le certificat de médecin UVG Suva</xhtml:p>
		<xhtml:h3 lang="it_ch">Certificato medico Suva LAINF: CDA Header</xhtml:h3>
		<xhtml:p lang="it_ch">Le regole seguenti descrivono i controlli 'CDA Header' per il certificato medico LAINF Suva</xhtml:p>
		<xhtml:h3 lang="en">Suva LAA medical certificate: CDA Header</xhtml:h3>
		<xhtml:p lang="en">The following rules describe the CDA Header tests for the Suva medical certificate</xhtml:p>
		<!-- Formularnummer prüfen -->
		<assert id="suva-form-2927-0101" test="//cda:templateId/@extension=&quot;2927&quot;">
			<xhtml:p lang="de_ch">Die Formularnummer für Suva Arztzeugnis UVG muss lauten: 2927</xhtml:p>
			<xhtml:p lang="fr_ch">Le numéro de formulaire pour le rapport médical initial LAA de la Suva doit être: 2927</xhtml:p>
			<xhtml:p lang="it_ch">Il numero form. del certificato medico LAINF Suva deve essere: 2927</xhtml:p>
			<xhtml:p lang="en">The form number for the Suva LAA medical certificate must read: 2927</xhtml:p>
		</assert>
		<!-- code prüfen -->
		<assert id="suva-form-2927-0102" test="//cda:code/@code=&quot;51848-0&quot;">
			<xhtml:p lang="de_ch">Der LOINC Code für das Suva Arztzeugnis UVG muss lauten: 51848-0 (Assessment note)</xhtml:p>
			<xhtml:p lang="fr_ch">Le code LOINC pour le rapport médical initial LAA de la Suva doit être: 51848-0 (Assessment note)</xhtml:p>
			<xhtml:p lang="it_ch">Il codice LOINC del certificato medico LAINF Suva deve essere: 51848-0 (Assessment note)</xhtml:p>
			<xhtml:p lang="en">The LOINC Code for the Suva LAA medical certificate must read: 51848-0 (Assessment note)</xhtml:p>
		</assert>
		<!-- title prüfen -->
		<assert id="suva-form-2927-0103" test="(//cda:title=&quot;Arztzeugnis UVG&quot; and //cda:languageCode/@code=&quot;de-CH&quot;) or //cda:languageCode/@code!=&quot;de-CH&quot;">
			<xhtml:p lang="de_ch">Der deutsche Titel muss lauten 'Arztzeugnis UVG'</xhtml:p>
			<xhtml:p lang="fr_ch">Le titre allemand doit être 'Arztzeugnis UVG'</xhtml:p>
			<xhtml:p lang="it_ch">Il titolo tedesco deve essere 'Arztzeugnis UVG'</xhtml:p>
			<xhtml:p lang="en">The German text must read 'Arztzeugnis UVG'</xhtml:p>
		</assert>
		<assert id="suva-form-2927-0104" test="(//cda:title=&quot;Rapport médical initial LAA&quot; and //cda:languageCode/@code=&quot;fr-CH&quot;) or //cda:languageCode/@code!=&quot;fr-CH&quot;">
			<xhtml:p lang="de_ch">Der französische Titel muss lauten 'Rapport médical initial LAA'</xhtml:p>
			<xhtml:p lang="fr_ch">Le titre français doit être 'Rapport médical initial LAA'</xhtml:p>
			<xhtml:p lang="it_ch">Il titolo francese deve essere 'Rapport médical initial LAA'</xhtml:p>
			<xhtml:p lang="en">The French title must read 'Rapport médical initial LAA'</xhtml:p>
		</assert>
		<assert id="suva-form-2927-0105" test="(//cda:title=&quot;Certificato medico LAINF&quot; and //cda:languageCode/@code=&quot;it-CH&quot;) or //cda:languageCode/@code!=&quot;it-CH&quot;">
			<xhtml:p lang="de_ch">Der italienische Titel muss lauten 'Certificato medico LAINF'</xhtml:p>
			<xhtml:p lang="fr_ch">Le titre italien doit être 'Certificato medico LAINF'</xhtml:p>
			<xhtml:p lang="it_ch">Il titolo italiano deve essere 'Certificato medico LAINF'</xhtml:p>
			<xhtml:p lang="en">The Italian title must read 'Certificato medico LAINF'</xhtml:p>
		</assert>
		<assert id="suva-form-2927-0106" test="(//cda:title=&quot;LAA medical certificate&quot; and //cda:languageCode/@code=&quot;en&quot;) or //cda:languageCode/@code!=&quot;en&quot;">
			<xhtml:p lang="de_ch">Der englische Titel muss lauten 'LAA medical certificate'</xhtml:p>
			<xhtml:p lang="fr_ch">Le titre anglais doit être 'LAA medical certificate'</xhtml:p>
			<xhtml:p lang="it_ch">Il titolo inglese deve essere 'LAA medical certificate'</xhtml:p>
			<xhtml:p lang="en">The English title must read 'LAA medical certificate'</xhtml:p>
		</assert>
		<!-- Arbeitgeber prüfen -->
		<assert id="suva-form-2927-0107" test="//cda:participant/cda:templateId[@root=&quot;2.16.756.5.30.1.1.1.1.1&quot; and @extension=&quot;CDA-CH.Head.Empl&quot;]">
			<xhtml:p lang="de_ch">Das Suva Arztzeugnis UVG muss einen Arbeitgeber enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical initial LAA de la Suva doit contenir un employeur</xhtml:p>
			<xhtml:p lang="it_ch">Nel certificato medico LAINF Suva deve essere indicato un datore di lavoro</xhtml:p>
			<xhtml:p lang="en">The Suva LAA medical certificate must contain an employer</xhtml:p>
		</assert>
		<!--
		********************************************************
		Aufbau des Dokuments (Existenz der Sections prüfen)
		********************************************************
		-->
		<assert id="suva-form-2927-0108" test="//cda:templateId/@root=&quot;2.16.756.5.30.1.100.2.2.1&quot; and //cda:templateId/@extension=&quot;eMedidoc.SchadenInfo&quot;">
			<xhtml:p lang="de_ch">Das Suva Arztzeugnis UVG muss eine Section 'Schadeninformationen' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical initial LAA de la Suva doit contenir une section 'Informations sur le sinistre'</xhtml:p>
			<xhtml:p lang="it_ch">Il certificato medico LAINF Suva deve contenere una sezione 'Informazioni relative al sinistro'</xhtml:p>
			<xhtml:p lang="en">The Suva LAA medical certificate must contain a 'Claim information' section</xhtml:p>
		</assert>
		<assert id="suva-form-2927-0109" test="//cda:templateId/@root=&quot;2.16.756.5.30.1.100.2&quot; and //cda:templateId/@extension=&quot;Suva.2927.AngabenPat&quot;">
			<xhtml:p lang="de_ch">Das Suva Arztzeugnis UVG muss eine Section 'Angaben des Patienten' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical initial LAA de la Suva doit contenir une section 'Indications du patient'</xhtml:p>
			<xhtml:p lang="it_ch">Il certificato medico LAINF Suva deve contenere una sezione 'Dichiarazioni del paziente'</xhtml:p>
			<xhtml:p lang="en">The Suva LAA medical certificate must contain an 'Information from patient' section</xhtml:p>
		</assert>
		<assert id="suva-form-2927-0110" test="//cda:templateId/@root=&quot;2.16.756.5.30.1.100.2&quot; and //cda:templateId/@extension=&quot;Suva.2927.Allgemeinzustand&quot;">
			<xhtml:p lang="de_ch">Das Suva Arztzeugnis UVG muss eine Section 'Allgemeinzustand' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical initial LAA de la Suva doit contenir une section 'Etat général'</xhtml:p>
			<xhtml:p lang="it_ch">Il certificato medico LAINF Suva deve contenere una sezione 'Stato generale'</xhtml:p>
			<xhtml:p lang="en">The Suva LAA medical certificate must contain a 'General condition' section</xhtml:p>
		</assert>
		<assert id="suva-form-2927-0111" test="//cda:templateId/@root=&quot;2.16.756.5.30.1.100.2&quot; and //cda:templateId/@extension=&quot;Suva.2927.Befund&quot;">
			<xhtml:p lang="de_ch">Das Suva Arztzeugnis UVG muss eine Section 'Befund' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical initial LAA de la Suva doit contenir une section 'Constatations'</xhtml:p>
			<xhtml:p lang="it_ch">Il certificato medico LAINF Suva deve contenere una sezione 'Referto' </xhtml:p>
			<xhtml:p lang="en">The Suva LAA medical certificate must contain a 'Findings' section</xhtml:p>
		</assert>
		<assert id="suva-form-2927-0112" test="//cda:templateId/@root=&quot;2.16.756.5.30.1.1.1.1.1&quot; and //cda:templateId/@extension=&quot;CDA-CH.Body.DiagList&quot;">
			<xhtml:p lang="de_ch">Das Suva Arztzeugnis UVG muss eine Section 'Diagnosen' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical initial LAA de la Suva doit contenir une section 'Diagnostics'</xhtml:p>
			<xhtml:p lang="it_ch">Il certificato medico LAINF Suva deve contenere una sezione 'Diagnosi'</xhtml:p>
			<xhtml:p lang="en">The Suva LAA medical certificate must contain a 'Diagnoses' section</xhtml:p>
		</assert>
		<assert id="suva-form-2927-0113" test="//cda:templateId/@root=&quot;2.16.756.5.30.1.100.2&quot; and //cda:templateId/@extension=&quot;Suva.2927.Kausalität&quot;">
			<xhtml:p lang="de_ch">Das Suva Arztzeugnis UVG muss eine Section 'Kausalität' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical initial LAA de la Suva de la Suva doit contenir une section 'Causalité'</xhtml:p>
			<xhtml:p lang="it_ch">Il certificato medico LAINF Suva deve contenere una sezione 'Causalità'</xhtml:p>
			<xhtml:p lang="en">The Suva LAA medical certificate must contain a 'Causality' section</xhtml:p>
		</assert>
		<assert id="suva-form-2927-0114" test="//cda:templateId/@root=&quot;2.16.756.5.30.1.100.2&quot; and //cda:templateId/@extension=&quot;Suva.2927.Therapie&quot;">
			<xhtml:p lang="de_ch">Das Suva Arztzeugnis UVG muss eine Section 'Therapie' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical initial LAA de la Suva doit contenir une section 'Thérapie'</xhtml:p>
			<xhtml:p lang="it_ch">Il certificato medico LAINF Suva deve contenere una sezione 'Terapia'</xhtml:p>
			<xhtml:p lang="en">The Suva LAA medical certificate must contain a 'Therapy' section</xhtml:p>
		</assert>
		<assert id="suva-form-2927-0115" test="//cda:templateId/@root=&quot;2.16.756.5.30.1.1.1.1.1&quot; and //cda:templateId/@extension=&quot;CDA-CH.Body.ArbeitsfähigkeitList&quot;">
			<xhtml:p lang="de_ch">Das Suva Arztzeugnis UVG muss eine Section 'Arbeitsfähigkeit' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical initial LAA de la Suva doit contenir une section 'Capacité de travail'</xhtml:p>
			<xhtml:p lang="it_ch">Il certificato medico LAINF Suva deve contenere una sezione 'Capacità lavorativa'</xhtml:p>
			<xhtml:p lang="en">The Suva LAA medical certificate must contain a 'Fitness for work' section</xhtml:p>
		</assert>
		<assert id="suva-form-2927-0116" role="information" test="//cda:templateId/@root=&quot;2.16.756.5.30.1.100.2.2.1&quot; and //cda:templateId/@extension=&quot;eMedidoc.BehAbschluss&quot;">
			<xhtml:p lang="de_ch">Das Suva Arztzeugnis UVG enthält keine Section 'Behandlungsabschluss'</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical initial LAA de la Suva ne contient pas de section 'Traitement terminé'</xhtml:p>
			<xhtml:p lang="it_ch">Il certificato medico LAINF Suva non contiene una sezione 'Conclusione del trattamento'</xhtml:p>
			<xhtml:p lang="en">The Suva LAA medical certificate does not contain an 'End of treatment' section</xhtml:p>
		</assert>
		<assert id="suva-form-2927-0117" test="//cda:templateId/@root=&quot;2.16.756.5.30.1.100.2.2.1&quot; and //cda:templateId/@extension=&quot;eMedidoc.Rem&quot;">
			<xhtml:p lang="de_ch">Das Suva Arztzeugnis UVG muss eine Section 'Bemerkungen' enthalten (mit Erwerbstatus. Wenn keine Bemerkungen, dann 'Keine' angeben)</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical initial LAA de la Suva doit contenir une section 'Remarques' (même s'il n'y a pas de remarques – indiquer alors 'Aucune')</xhtml:p>
			<xhtml:p lang="it_ch">Il certificato medico LAINF Suva deve contenere una sezione 'Osservazioni' (con situazione lavorativa. Se non ci sono osservazioni, scrivere 'Nessuna')</xhtml:p>
			<xhtml:p lang="en">The Suva LAA medical certificate must contain a 'Remarks' section (with employment status. If no remarks, then enter 'None')</xhtml:p>
		</assert>
	</rule>
	<!--
	********************************************************
	Arztzeugnis UVG: Schadeninformationen
	********************************************************
	-->
	<rule context="*[cda:templateId/@root=&quot;2.16.756.5.30.1.100.2.2.1&quot; and cda:templateId/@extension=&quot;eMedidoc.SchadenInfo&quot;]">
		<!-- Rule Documentation -->
		<xhtml:h3 lang="de_ch">Arztzeugnis UVG: Schadeninformationen</xhtml:h3>
		<xhtml:p lang="de_ch">Die nachfolgenden Regeln beschreiben die Prüfungen zu 'Schadeninformationen' des Arztzeugnis UVG</xhtml:p>
		<xhtml:h3 lang="fr_ch">Rapport médical initial LAA: Informations sur le sinistre</xhtml:h3>
		<xhtml:p lang="fr_ch">Les règles suivantes décrivent les contrôles sur 'Informations sur le sinistre' pour le certificat de médecin LAA</xhtml:p>
		<xhtml:h3 lang="it_ch">Certificato medico LAINF: informazioni relative al sinistro</xhtml:h3>
		<xhtml:p lang="it_ch">Le regole seguenti descrivono i controlli per le 'Informazioni relative al sinistro' del certificato medico LAINF</xhtml:p>
		<xhtml:h3 lang="en">LAA medical certificate: Claim information</xhtml:h3>
		<xhtml:p lang="en">The following rules describe the tests on 'Claim information' for the LAA medical certificate</xhtml:p>
		<assert id="suva-form-2927-0201" test="descendant::*[cda:templateId/@root=&quot;2.16.756.5.30.1.100.2.2.1&quot; and cda:templateId/@extension=&quot;eMedidoc.ErstbehandlungL3&quot;]">
			<xhtml:p lang="de_ch">eMedidoc 'Schadeninformationen' müssen Angaben zur Erstbehandlung enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Les 'Informations sur le sinistre' eMedidoc doivent contenir des informations sur le traitement initial</xhtml:p>
			<xhtml:p lang="it_ch">Le 'Informazioni relative al sinistro' eMedidoc devono contenere informazioni sul trattamento iniziale</xhtml:p>
			<xhtml:p lang="en">eMedidoc 'Claim information' must contain information about the initial treatment</xhtml:p>
		</assert>
	</rule>
	<rule context="*[cda:templateId/@root=&quot;2.16.756.5.30.1.100.2.2.1.1.1.1&quot;]">
		<!-- Rule Documentation -->
		<xhtml:h3 lang="de_ch">Arztzeugnis UVG: Schadeninformationen (Angaben zur Erstbehandlung)</xhtml:h3>
		<xhtml:p lang="de_ch">Die nachfolgenden Regeln beschreiben die Prüfungen zu 'Schadeninformationen, Angaben zur Erstbehandlung' des Arztzeugnis UVG</xhtml:p>
		<xhtml:h3 lang="fr_ch">Rapport médical initial LAA: Informations sur le sinistre (informations sur le traitement initial)</xhtml:h3>
		<xhtml:p lang="fr_ch">Les règles suivantes décrivent les contrôles sur 'Informations sur le sinistre, informations sur le traitement initial' pour le certificat de médecin LAA</xhtml:p>
		<xhtml:h3 lang="it_ch">Certificato medico LAINF: informazioni relative al sinistro (indicazioni sul trattamento iniziale)</xhtml:h3>
		<xhtml:p lang="it_ch">Le regole seguenti descrivono i controlli per le 'Informazioni relative al sinistro, indicazioni sul trattamento iniziale' del certificato medico LAINF</xhtml:p>
		<xhtml:h3 lang="en">LAA medical certificate: Claim information (information about the initial treatment)</xhtml:h3>
		<xhtml:p lang="en">The following rules describe tests on 'Claim information, information about the initial treatment' for the LAA medical certificate</xhtml:p>
		<assert id="suva-form-2927-0301" test="descendant::*[cda:templateId/@root=&quot;2.16.756.5.30.1.100.2.2.1.1.3.1&quot;]">
			<xhtml:p lang="de_ch">Das Arztzeugnis UVG muss bei den Schadeninformationen die Angaben zur Erstbehandlung enthalten (templateId 2.16.756.5.30.1.100.2.2.1.1.3.1)</xhtml:p>
			<xhtml:p lang="fr_ch">Le rapport médical initial LAA doit contenir les indications sur le traitement initial dans les informations sur le sinistre (templateId 2.16.756.5.30.1.100.2.2.1.1.3.1)</xhtml:p>
			<xhtml:p lang="it_ch">Il certificato medico LAINF deve contenere nelle informazioni relative al sinistro le indicazioni sul trattamento iniziale (templateId 2.16.756.5.30.1.100.2.2.1.1.3.1)</xhtml:p>
			<xhtml:p lang="en">The LAA medical certificate must contain information about the initial treatment in the claim information (templateId 2.16.756.5.30.1.100.2.2.1.1.3.1)</xhtml:p>
		</assert>
	</rule>
	<!--
	********************************************************
	Arztzeugnis UVG: Angaben des Patienten
	********************************************************
	-->
	<rule context="*[cda:templateId/@root=&quot;2.16.756.5.30.1.100.2&quot; and cda:templateId/@extension=&quot;Suva.2927.AngabenPat&quot;]">
		<!-- Rule Documentation -->
		<xhtml:h3 lang="de_ch">Arztzeugnis UVG: Angaben des Patienten</xhtml:h3>
		<xhtml:p lang="de_ch">Die nachfolgenden Regeln beschreiben die Prüfungen zu 'Angaben des Patienten' des Arztzeugnis UVG</xhtml:p>
		<xhtml:h3 lang="fr_ch">Rapport médical initial LAA: Indications du patient</xhtml:h3>
		<xhtml:p lang="fr_ch">Les règles suivantes décrivent les contrôles sur 'Indications du patient' pour le certificat de médecin LAA</xhtml:p>
		<xhtml:h3 lang="it_ch">Certificato medico LAINF: dichiarazioni del paziente</xhtml:h3>
		<xhtml:p lang="it_ch">Le regole seguenti descrivono i controlli per le 'Dichiarazioni del paziente' del certificato medico LAINF</xhtml:p>
		<xhtml:h3 lang="en">LAA medical certificate: Information from the patient</xhtml:h3>
		<xhtml:p lang="en">The following rules describe the tests on 'Information from the patient' in the LAA medical certificate</xhtml:p>
		<assert id="suva-form-2927-0401" test="self::cda:section">
			<xhtml:p lang="de_ch">eMedidoc 'Angaben des Patienten' müssen als 'section' im CDA Body deklariert werden</xhtml:p>
			<xhtml:p lang="fr_ch">Les 'Indications du patient' eMedidoc doivent être déclarées comme 'section' dans le CDA Body</xhtml:p>
			<xhtml:p lang="it_ch">Le 'Dichiarazioni del paziente' eMedidoc devono essere dichiarate come 'section' nel CDA Body</xhtml:p>
			<xhtml:p lang="en">eMedidoc 'Information from the patient' must be declaired as a 'Section' in the CDA Body</xhtml:p>
		</assert>
		<assert id="suva-form-2927-0402" test="cda:text">
			<xhtml:p lang="de_ch">eMedidoc 'Angaben des Patienten' müssen einen narrativen Text enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Les 'Indications du patient' eMedidoc doivent contenir un texte narratif</xhtml:p>
			<xhtml:p lang="it_ch">Le 'Dichiarazioni del paziente' eMedidoc devono contenere un testo narrativo</xhtml:p>
			<xhtml:p lang="en">eMedidoc 'Information from the patient' must contain a narrative text</xhtml:p>
		</assert>
		<assert id="suva-form-2927-0403" test="(cda:title=&quot;Angaben des Patienten&quot; and //cda:languageCode/@code=&quot;de-CH&quot;) or //cda:languageCode/@code!=&quot;de-CH&quot;">
			<xhtml:p lang="de_ch">Deutsch: eMedidoc 'Angaben des Patienten' müssen einen section title 'Angaben des Patienten' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Allemand: Les 'Indications du patient' eMedidoc doivent contenir un section title 'Angaben des Patienten'</xhtml:p>
			<xhtml:p lang="it_ch">Tedesco: Le 'Dichiarazioni del paziente' eMedidoc devono contenere un section title 'Angaben des Patienten'</xhtml:p>
			<xhtml:p lang="en">German: eMedidoc 'Information from the patient' must contain an 'Angaben des Patienten' section title</xhtml:p>
		</assert>
		<assert id="suva-form-2927-0404" test="(cda:title=&quot;Indications du patient&quot; and //cda:languageCode/@code=&quot;fr-CH&quot;) or //cda:languageCode/@code!=&quot;fr-CH&quot;">
			<xhtml:p lang="de_ch">Französisch: eMedidoc 'Angaben des Patienten' müssen einen section title 'Indications du patient' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Français: Les 'Indications du patient' eMedidoc doivent contenir un section title 'Indications du patient'</xhtml:p>
			<xhtml:p lang="it_ch">Francese: Le 'Dichiarazioni del paziente' eMedidoc devono contenere un section title 'Indications du patient'</xhtml:p>
			<xhtml:p lang="en">French: eMedidoc 'Information form the patient' must contain an 'Indications du patient' section title</xhtml:p>
		</assert>
		<assert id="suva-form-2927-0405" test="(cda:title=&quot;Dichiarazioni del paziente&quot; and //cda:languageCode/@code=&quot;it-CH&quot;) or //cda:languageCode/@code!=&quot;it-CH&quot;">
			<xhtml:p lang="de_ch">Italienisch: eMedidoc 'Angaben des Patienten' müssen einen section title 'Dichiarazioni del paziente' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Italien: Les 'Indications du patient' eMedidoc doivent contenir un section title 'Dichiarazioni del paziente'</xhtml:p>
			<xhtml:p lang="it_ch">Italiano: Le 'Dichiarazioni del paziente' eMedidoc devono contenere un section title 'Dichiarazioni del paziente'</xhtml:p>
			<xhtml:p lang="en">Italian: eMedidoc 'Information form the patient' must contain an 'Dichiarazioni del paziente' section title</xhtml:p>
		</assert>
