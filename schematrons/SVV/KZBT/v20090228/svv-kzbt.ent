	********************************************************
	-->
	<rule context="*[cda:templateId/@root=&quot;2.16.756.5.30.1.110.2.1.1&quot; and cda:templateId/@extension=&quot;SVV.KZBT.SchadenInfo&quot;]">
		<!-- Rule Documentation -->
		<xhtml:h3 lang="de_ch">SVV KZBT: Schadeninformationen</xhtml:h3>
		<xhtml:p lang="de_ch">Die nachfolgenden Regeln beschreiben die Prüfungen für 'Schadeninformationen'</xhtml:p>

		<xhtml:h3 lang="fr_ch">ASA TACC: information sur le sinistre</xhtml:h3>
		<xhtml:p lang="fr_ch">Les règles suivantes décrivent les contrôles pour 'information sur le sinistre' </xhtml:p>

		<xhtml:h3 lang="it_ch"></xhtml:h3>
		<xhtml:p lang="it_ch"></xhtml:p>

		<xhtml:h3 lang="en">SIA WI: Claim information</xhtml:h3>
		<xhtml:p lang="en">The following rules describe the tests on 'Claim information'</xhtml:p>

		<assert id="svv-kzbt-0201" test="self::cda:section">
			<xhtml:p lang="de_ch">Schadeninformationen müssen als 'section' im CDA Body deklariert werden</xhtml:p>
			<xhtml:p lang="fr_ch">Les informations sur le sinistre doivent être déclarées comme 'section' dans le CDA Body</xhtml:p>
			<xhtml:p lang="it_ch"></xhtml:p>
			<xhtml:p lang="en">Claim information must be declared as a 'Section' in the CDA Body</xhtml:p>
		</assert>
		<assert id="svv-kzbt-0202" test="cda:text">
			<xhtml:p lang="de_ch">Schadeninformationen müssen einen narrativen Text enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Les informations sur le sinistre doivent contenir un texte narratif</xhtml:p>
			<xhtml:p lang="it_ch"></xhtml:p>
			<xhtml:p lang="en">Claim information must contain a narrative text</xhtml:p>
		</assert>
		<assert id="svv-kzbt-0203" test="(cda:title=&quot;Schadeninformationen&quot; and //cda:languageCode/@code=&quot;de-CH&quot;) or //cda:languageCode/@code!=&quot;de-CH&quot;">
			<xhtml:p lang="de_ch">Deutsch: Schadeninformationen müssen einen section title 'Schadeninformationen' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Allemand: Les informations sur le sinistre doivent contenir un section title 'Schadeninformationen'</xhtml:p>
			<xhtml:p lang="it_ch"></xhtml:p>
			<xhtml:p lang="en">English: Claim information must contain a 'Schadeninformationen' section title</xhtml:p>
		</assert>
		<assert id="svv-kzbt-0204" test="(cda:title=&quot;Information sur le sinistre&quot; and //cda:languageCode/@code=&quot;fr-CH&quot;) or //cda:languageCode/@code!=&quot;fr-CH&quot;">
			<xhtml:p lang="de_ch">Französisch: Schadeninformationen müssen einen section title 'Information sur le sinistre' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Français: Les informations sur le sinistre doivent contenir un section title 'Information sur le sinistre'</xhtml:p>
			<xhtml:p lang="it_ch"></xhtml:p>
			<xhtml:p lang="en">French: Claim information must contain a 'Claim information' section title</xhtml:p>
		</assert>
		<assert id="svv-kzbt-0204" test="(cda:title=&quot;Informazioni relative al sinistro&quot; and //cda:languageCode/@code=&quot;it-CH&quot;) or //cda:languageCode/@code!=&quot;it-CH&quot;">
			<xhtml:p lang="de_ch">Italienisch: Schadeninformationen müssen einen section title 'Informazioni relative al sinistro' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Italien: Les informations sur le sinistre doivent contenir un section title 'Informazioni relative al sinistro'</xhtml:p>
			<xhtml:p lang="it_ch"></xhtml:p>
			<xhtml:p lang="en">Italian: Claim information must contain a 'Informazioni relative al sinistro' section title</xhtml:p>
		</assert>
		<assert id="svv-kzbt-0204" test="(cda:title=&quot;Claim information&quot; and //cda:languageCode/@code=&quot;en&quot;) or //cda:languageCode/@code!=&quot;en&quot;">
			<xhtml:p lang="de_ch">Englisch: Schadeninformationen müssen einen section title 'Claim information' enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Anglais: Les informations sur le sinistre doivent contenir un section title 'Claim information'</xhtml:p>
			<xhtml:p lang="it_ch"></xhtml:p>
			<xhtml:p lang="en">English: Claim information must contain a 'Claim information' section title</xhtml:p>
		</assert>
		<assert id="svv-kzbt-0205" test="descendant::*[cda:templateId/@root=&quot;2.16.756.5.30.1.110.2.1.1&quot; and cda:templateId/@extension=&quot;SVV.KZBT.SchadenInfoL3&quot;]">
			<xhtml:p lang="de_ch">Schadeninformationen müssen das Schadendatum enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Les informations sur le sinistre doivent contenir la date de l’accident</xhtml:p>
			<xhtml:p lang="it_ch"></xhtml:p>
			<xhtml:p lang="en">Claim information must contain a claim date</xhtml:p>
		</assert>
		<assert id="svv-kzbt-0206" test="descendant::*[cda:templateId/@root=&quot;2.16.756.5.30.1.110.2.1.1&quot; and cda:templateId/@extension=&quot;SVV.KZBT.ErstbehandlungL3&quot;]">
			<xhtml:p lang="de_ch">SVV KZBT 'Schadeninformationen' müssen Angaben zur Erstbehandlung enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Les 'Information sur le sinistre' ASA TACC doivent contenir des indications sur le traitement initial</xhtml:p>
			<xhtml:p lang="it_ch"></xhtml:p>
			<xhtml:p lang="en">SIA WI 'Claim information' must contain information about the intial treatment</xhtml:p>
		</assert>
		<assert id="svv-kzbt-0207" test="descendant::*[cda:templateId/@root=&quot;2.16.756.5.30.1.110.2.1.1&quot; and cda:templateId/@extension=&quot;SVV.KZBT.AmbulanzL3&quot;]">
			<xhtml:p lang="de_ch">SVV KZBT 'Schadeninformationen' müssen Angaben zur Ambulanz enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Les 'Information sur le sinistre' ASA TACC doivent contenir des indications sur l’ambulance</xhtml:p>
			<xhtml:p lang="it_ch"></xhtml:p>
			<xhtml:p lang="en">SIA WI 'Claim information' must contain information on ambulance</xhtml:p>
		</assert>
		<assert id="svv-kzbt-0208" test="descendant::*[cda:templateId/@root=&quot;2.16.756.5.30.1.110.2.1.1&quot; and cda:templateId/@extension=&quot;SVV.KZBT.WeiterbehandlungL3&quot;]">
			<xhtml:p lang="de_ch">SVV KZBT 'Schadeninformationen' müssen Angaben zur Weiterbehandlung enthalten</xhtml:p>
			<xhtml:p lang="fr_ch">Les 'Information sur le sinistre' ASA TACC doivent contenir des indications sur la poursuite du traitement</xhtml:p>
			<xhtml:p lang="it_ch"></xhtml:p>
			<xhtml:p lang="en">SIA WI 'Claim information' must contain information about further treatment</xhtml:p>
		</assert>
	</rule>
