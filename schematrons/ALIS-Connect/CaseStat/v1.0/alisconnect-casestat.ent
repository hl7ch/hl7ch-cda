<!--
********************************************************

Schematron rules for ALIS-Connect case statistics (casestat) V1.0 (2.16.756.5.30.PlaceholderCaseStatV1)
Common content rules for GrpInfo and GrpStatus

TODOs:
- Temporäre OIDs ersetzen


History:
13.03.2011: Tony Schaller, medshare GmbH (Initial draft)
20.03.2011: Tony Schaller, medshare GmbH (Abgabe an Auftraggeber zum Review)
25.03.2011: Tony Schaller, medshare GmbH (Abgabe an Projektteam ALIS-Connect CaseStat zum Review)
06.04.2011: Tony Schaller, medshare GmbH (Publikation in SVN Repository der HL7 Benutzergruppe Schweiz; Achtung: OIDs vorerst als Platzhalter)

********************************************************
-->
<!--
********************************************************
Document Templates
********************************************************
-->
<pattern>
	<!-- Entity Information -->
	<xhtml:ul id="entity-alisconnect-casestat" xmlns:xhtml="http://www.w3.org/1999/xhtml">
		<xhtml:li class="filename">alisconnect-casestat.ent</xhtml:li>
		<xhtml:li class="version">1.0</xhtml:li>
	</xhtml:ul>
	<!--
	********************************************************
	Externe Templates
	********************************************************
	Soziodemographische Angaben Patient:
	- Geschlecht: entity_HL7-cda-ch_vitalsigns-0001
	- Geburtsdatum: entity_HL7-cda-ch_vitalsigns-0004
	-->
</pattern>
<!--
********************************************************
Header Templates
********************************************************
-->
<pattern>
	<rule context="*[cda:templateId/@root=&quot;2.16.756.5.30.PlaceholderCaseStatV1&quot; and (cda:templateId/@extension=&quot;alisconnect.casestat-grpinfo.doc&quot; or cda:templateId/@extension=&quot;alisconnect.casestat-grpstatus.doc&quot;)]">
		<!-- Rule Documentation -->
		<xhtml:h3 lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">ALIS-Connect Case Statistics (casestat): CDA Header Templates</xhtml:h3>
		<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Die nachfolgenden Regeln beschreiben die Prüfungen zum CDA Header von ALIS-Connect CaseStat Dokumenten</xhtml:p>
		<!-- Soziodemographische Angaben Patient: Wohnort -->
		<assert role="warning" id="entity-alisconnect-casestat-0101" test="count(cda:recordTarget/cda:patientRole/cda:addr/cda:city)=1">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">ALIS-Connect CaseStat Header: Soziodemographische Angaben Patient: Wohnort sollte angegeben werden</xhtml:p>
		</assert>
		<!-- Versicherung -->
		<assert role="warning" id="entity-alisconnect-casestat-0102" test="cda:participant[@typeCode=&quot;COV&quot;]/cda:templateId[@root=&quot;2.16.756.5.30.1.1.1.1.1&quot; and @extension=&quot;CDA-CH.Head.Insurance&quot;]">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">ALIS-Connect CaseStat Header: Versicherung sollte angegeben werden (mit Template 'CDA-CH.Head.Insurance')</xhtml:p>
		</assert>
	</rule>
</pattern>
<!--
********************************************************
Body Templates
********************************************************
-->
<pattern>
	<!--
	********************************************************
	Soziodemographie: Nationalität
	********************************************************
	-->
	<rule context="*[cda:templateId/@root=&quot;2.16.756.5.30.PlaceholderCaseStatV1&quot; and cda:templateId/@extension=&quot;alisconnect.casestat.body.Soziodemographie&quot;]">
		<!-- Rule Documentation -->
		<xhtml:h3 lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">ALIS-Connect Case Statistics (casestat): CDA Body Template 'Soziodemographie'</xhtml:h3>
		<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Die nachfolgenden Regeln beschreiben die Prüfungen zum CDA Body Element 'Soziodemographie'</xhtml:p>
		<assert id="entity-alisconnect-casestat-0201" test="(cda:title=&quot;Soziodemographische Angaben&quot; and //cda:languageCode/@code=&quot;de-CH&quot;) or //cda:languageCode/@code!=&quot;de-CH&quot;">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Soziodemographische Angaben Patient müssen einen section title 'Soziodemographische Angaben' enthalten</xhtml:p>
		</assert>
		<assert id="entity-alisconnect-casestat-0202" test="cda:entry/cda:observation/cda:templateId[@root=&quot;2.16.756.5.30.1.1.1.1.1&quot; and @extension=&quot;CDA-CH.Body.NationalityL3&quot;]">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Soziodemographische Angaben Patient: Nationalität muss mit dem Template 'CDA-CH.Body.NationalityL3' deklariert werden</xhtml:p>
		</assert>
	</rule>
</pattern>
<pattern>
	<!--
	********************************************************
	Eintrittsmerkmale
	********************************************************
	-->
	<rule context="*[cda:templateId/@root=&quot;2.16.756.5.30.PlaceholderCaseStatV1&quot; and cda:templateId/@extension=&quot;alisconnect.casestat.body.Eintrittsmerkmale&quot;]">
		<!-- Rule Documentation -->
		<xhtml:h3 lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">ALIS-Connect Case Statistics (casestat): CDA Body Template 'Eintrittsmerkmale'</xhtml:h3>
		<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Die nachfolgenden Regeln beschreiben die Prüfungen zum CDA Body Element 'Eintrittsmerkmale'</xhtml:p>
		<assert id="entity-alisconnect-casestat-0301" test="(cda:title=&quot;Eintrittsmerkmale&quot; and //cda:languageCode/@code=&quot;de-CH&quot;) or //cda:languageCode/@code!=&quot;de-CH&quot;">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Eintrittsmerkmale müssen einen section title 'Eintrittsmerkmale' enthalten</xhtml:p>
		</assert>
		<assert id="entity-alisconnect-casestat-0302" test="cda:entry/cda:encounter/cda:templateId[@root=&quot;2.16.756.5.30.PlaceholderCaseStatV1&quot; and @extension=&quot;alisconnect.casestat.body.EintrittL3&quot;]">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Eintrittsmerkmale: Eintritt muss mit dem Template 'alisconnect.casestat.body.EintrittL3' deklariert werden</xhtml:p>
		</assert>
		<assert id="entity-alisconnect-casestat-0303" test="cda:entry/cda:observation/cda:templateId[@root=&quot;2.16.756.5.30.PlaceholderCaseStatV1&quot; and @extension=&quot;alisconnect.casestat.body.EintrittAngabenMSKL3&quot;]">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Eintrittsmerkmale: Angaben für MSK müssen mit dem Template 'alisconnect.casestat.body.EintrittAngabenMSKL3' deklariert werden</xhtml:p>
		</assert>
	</rule>
</pattern>
<pattern>
	<!--
	********************************************************
	Eintrittsmerkmale: Eintritt L3
	********************************************************
	-->
	<rule context="*[cda:templateId/@root=&quot;2.16.756.5.30.PlaceholderCaseStatV1&quot; and cda:templateId/@extension=&quot;alisconnect.casestat.body.EintrittL3&quot;]">
		<!-- Rule Documentation -->
		<xhtml:h3 lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">ALIS-Connect Case Statistics (casestat): CDA Body Level3 Template 'Eintritt'</xhtml:h3>
		<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Die nachfolgenden Regeln beschreiben die Prüfungen zum CDA Body Entry 'Eintritt'</xhtml:p>
		<!-- Deklaration als Encounter -->
		<assert id="entity-alisconnect-casestat-0401" test="self::cda:encounter">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Der Eintritt muss als 'encounter' im CDA Body (section 'Eintrittsmerkmale') deklariert werden</xhtml:p>
		</assert>
		<!-- Fallnummer -->
		<assert id="entity-alisconnect-casestat-0402" test="cda:id">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Eintritt: Die Fallnummer muss als 'id' deklariert werden</xhtml:p>
		</assert>
		<!-- MSK Eintrittsmerkmale: Eintrittsdatum und -Stunde -->
		<assert id="entity-alisconnect-casestat-0403" test="cda:effectiveTime">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Eintritt: Das Eintrittsdatum inkl. Stunde muss angegeben werden</xhtml:p>
		</assert>
		<!-- Ausfüllender Arzt -->
		<assert id="entity-alisconnect-casestat-0404" test="cda:performer/cda:assignedEntity/cda:assignedPerson/cda:name">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Eintritt: Es muss mindestens der Namen des Arztes, der den Eintrittsbericht verfasst hat angeben werden ('performer/assignedEntity/assignedPerson/name') (CDA Body Level 3)</xhtml:p>
		</assert>
	</rule>
</pattern>
<pattern>
	<!--
	********************************************************
	Eintrittsmerkmale: EintrittAngabenMSKL3
	********************************************************
	-->
	<rule context="*[cda:templateId/@root=&quot;2.16.756.5.30.PlaceholderCaseStatV1&quot; and cda:templateId/@extension=&quot;alisconnect.casestat.body.EintrittAngabenMSKL3&quot;]">
		<!-- Rule Documentation -->
		<xhtml:h3 lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">ALIS-Connect Case Statistics (casestat): CDA Body Level3 Template 'Eintritt Angaben MSK'</xhtml:h3>
		<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Die nachfolgenden Regeln beschreiben die Prüfungen zum CDA Body Entry 'Eintritt Angaben MSK'</xhtml:p>
		<!-- Deklaration als Observation -->
		<assert id="entity-alisconnect-casestat-0501" test="self::cda:observation">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Eintritt Angaben MSK: Der Eintritt muss als 'observation' im CDA Body (section 'Eintrittsmerkmale') deklariert werden</xhtml:p>
		</assert>
		<!-- MSK Eintrittsmerkmale: Eintrittsart -->
		<assert id="entity-alisconnect-casestat-0502" test="cda:code[@codeSystem=&quot;2.16.756.5.30.PlaceholderMSKEintrittsart&quot; and @code=document(&quot;../../../../schematrons/BFS/MSK/v201103/MSK-voc.xml&quot;)/systems/system[@root=&quot;2.16.756.5.30.PlaceholderMSKEintrittsart&quot;]/code/@value]">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Eintritt Angaben MSK: Der Eintritt muss die Angabe zur Eintrittsart enthalten (gültiger Wert aus der Wertetabelle '2.16.756.5.30.PlaceholderMSKEintrittsart')</xhtml:p>
		</assert>
		<assert id="entity-alisconnect-casestat-0503" test="count(cda:code)&lt;2">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Eintritt Angaben MSK: Die Angabe zur Eintrittsart darf nur einmal vorkommen</xhtml:p>
		</assert>
		<!-- MSK Eintrittsmerkmale: Aufenthalt vor Eintritt -->
		<assert id="entity-alisconnect-casestat-0504" test="cda:value[@xsi:type=&quot;CE&quot; and @codeSystem=&quot;2.16.756.5.30.PlaceholderMSKAufenthaltsortVorEintritt&quot; and @code=document(&quot;../../../../schematrons/BFS/MSK/v201103/MSK-voc.xml&quot;)/systems/system[@root=&quot;2.16.756.5.30.PlaceholderMSKAufenthaltsortVorEintritt&quot;]/code/@value]">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Eintritt Angaben MSK: Der Eintritt muss die Angabe zum Aufenthalt vor Eintritt enthalten (gültiger Wert aus der Wertetabelle '2.16.756.5.30.PlaceholderMSKAufenthaltsortVorEintritt')</xhtml:p>
		</assert>
		<assert id="entity-alisconnect-casestat-0505" test="count(cda:value[@xsi:type=&quot;CE&quot; and @codeSystem=&quot;2.16.756.5.30.PlaceholderMSKAufenthaltsortVorEintritt&quot;])&lt;2">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Eintritt Angaben MSK: Die Angabe zum Aufenthalt vor Eintritt darf nur einmal vorkommen</xhtml:p>
		</assert>
		<!-- MSK Eintrittsmerkmale: Einweisende Instanz -->
		<assert id="entity-alisconnect-casestat-0506" test="cda:value[@xsi:type=&quot;CE&quot; and @codeSystem=&quot;2.16.756.5.30.PlaceholderMSKEinweisendeInstanz&quot; and @code=document(&quot;../../../../schematrons/BFS/MSK/v201103/MSK-voc.xml&quot;)/systems/system[@root=&quot;2.16.756.5.30.PlaceholderMSKEinweisendeInstanz&quot;]/code/@value]">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Eintritt Angaben MSK: Der Eintritt muss die Angabe zur Einweisenden Instanz enthalten (gültiger Wert aus der Wertetabelle '2.16.756.5.30.PlaceholderMSKEinweisendeInstanz')</xhtml:p>
		</assert>
		<assert id="entity-alisconnect-casestat-0507" test="count(cda:value[@xsi:type=&quot;CE&quot; and @codeSystem=&quot;2.16.756.5.30.PlaceholderMSKEinweisendeInstanz&quot;])&lt;2">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Eintritt Angaben MSK: Die Angabe zur Einweisenden Instanz darf nur einmal vorkommen</xhtml:p>
		</assert>
	</rule>
</pattern>
<pattern>
	<!--
	********************************************************
	Aufenthaltsmerkmale
	********************************************************
	-->
	<rule context="*[cda:templateId/@root=&quot;2.16.756.5.30.PlaceholderCaseStatV1&quot; and cda:templateId/@extension=&quot;alisconnect.casestat.body.Aufenthaltsmerkmale&quot;]">
		<!-- Rule Documentation -->
		<xhtml:h3 lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">ALIS-Connect Case Statistics (casestat): CDA Body Template 'Aufenthaltsmerkmale'</xhtml:h3>
		<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Die nachfolgenden Regeln beschreiben die Prüfungen zum CDA Body Element 'Aufenthaltsmerkmale'</xhtml:p>
		<assert id="entity-alisconnect-casestat-0601" test="(cda:title=&quot;Aufenthaltsmerkmale&quot; and //cda:languageCode/@code=&quot;de-CH&quot;) or //cda:languageCode/@code!=&quot;de-CH&quot;">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Aufenthaltsmerkmale müssen einen section title 'Aufenthaltsmerkmale' enthalten</xhtml:p>
		</assert>
		<assert id="entity-alisconnect-casestat-0602" test="cda:entry/cda:observation/cda:templateId[@root=&quot;2.16.756.5.30.PlaceholderCaseStatV1&quot; and @extension=&quot;alisconnect.casestat.body.AufenthaltAngabenMSKL3&quot;]">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Aufenthaltsmerkmale: Angaben für MSK müssen mit dem Template 'alisconnect.casestat.body.AufenthaltAngabenMSKL3' deklariert werden</xhtml:p>
		</assert>
		<assert role="information" id="entity-alisconnect-casestat-0603" test="cda:entry/cda:encounter/cda:templateId[@root=&quot;2.16.756.5.30.PlaceholderCaseStatV1&quot; and @extension=&quot;alisconnect.casestat.body.IPSAufenthaltL3&quot;]">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Aufenthaltsmerkmale können Angaben für MSK zum Aufenthalt in einer Intensivstation enthalten (Template 'alisconnect.casestat.body.IPSAufenthaltL3')</xhtml:p>
		</assert>
	</rule>
</pattern>
<pattern>
	<!--
	********************************************************
	Aufenthaltsmerkmale: Aufenthalt in einer Intensivstation L3 (Gerüst zur Berechnung der vollendeten Stunden)
	********************************************************
	-->
	<rule context="*[cda:templateId/@root=&quot;2.16.756.5.30.PlaceholderCaseStatV1&quot; and cda:templateId/@extension=&quot;alisconnect.casestat.body.IPSAufenthaltL3&quot;]">
		<!-- Rule Documentation -->
		<xhtml:h3 lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">ALIS-Connect Case Statistics (casestat): CDA Body Level3 Template 'Aufenthalt in einer Intensivstation'</xhtml:h3>
		<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Die nachfolgenden Regeln beschreiben die Prüfungen zum CDA Body Entry 'Aufenthalt in einer Intensivstation'. Hinweis: Wo der Aufenthalt nur in Form von Stunden und nicht als Zeitraum (von/bis) verfügbar ist, im 'low value' ein Datum mit Zeitpunkt 00:00 Uhr und im 'high value' die Anzahl Stunden als Differenz zum 'low value' angeben</xhtml:p>
		<!-- Deklaration als Encounter -->
		<assert id="entity-alisconnect-casestat-0701" test="self::cda:encounter">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Aufenthalt in einer Intensivstation: Der Eintritt in die Intensivstation muss als 'encounter' im CDA Body (section 'Aufenthaltsmerkmale') deklariert werden</xhtml:p>
		</assert>
		<!-- Fallnummer -->
		<assert id="entity-alisconnect-casestat-0702" test="cda:id">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Aufenthalt in einer Intensivstation: Die Fallnummer muss als 'id' deklariert werden</xhtml:p>
		</assert>
		<!-- MSK Aufenthalt in einer Intensivstation: Eintrittsdatum und -Stunde -->
		<assert id="entity-alisconnect-casestat-0703" test="cda:effectiveTime/cda:low/@value">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Aufenthalt in einer Intensivstation: Der Eintritt in die Intensivstation muss inkl. Stunde deklariert werden (dient der Berechnung des IPS Aufenthaltes in der Codierabteilung)</xhtml:p>
		</assert>
		<!-- MSK Aufenthalt in einer Intensivstation: Austrittsdatum und -Stunde -->
		<assert id="entity-alisconnect-casestat-0704" test="cda:effectiveTime/cda:high/@value">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Aufenthalt in einer Intensivstation: Der Austritt aus der Intensivstation muss inkl. Stunde deklariert werden (dient der Berechnung des IPS Aufenthaltes in der Codierabteilung)</xhtml:p>
		</assert>
		<!-- Ausfüllender Arzt -->
		<assert id="entity-alisconnect-casestat-0705" test="cda:performer/cda:assignedEntity/cda:assignedPerson/cda:name">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Aufenthalt in einer Intensivstation: Es muss mindestens der Namen des Arztes, der den IPS Verlegungsbericht verfasst hat angeben werden ('performer/assignedEntity/assignedPerson/name') (CDA Body Level 3)</xhtml:p>
		</assert>
		<assert role="warning" id="entity-alisconnect-casestat-0706" test="cda:entryRelationship/cda:encounter/cda:templateId[@root=&quot;2.16.756.5.30.PlaceholderCaseStatV1&quot; and @extension=&quot;alisconnect.casestat.body.IPSAufenthaltBeatmungL3&quot;]">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Aufenthalt in einer Intensivstation kann Angaben für MSK zur künstlichen Beatmung in einer Intensivstation enthalten (Template 'alisconnect.casestat.body.IPSAufenthaltBeatmungL3')</xhtml:p>
		</assert>
		<assert id="entity-alisconnect-casestat-0707" test="cda:entryRelationship/cda:observation/cda:templateId[@root=&quot;2.16.756.5.30.PlaceholderCaseStatV1&quot; and @extension=&quot;alisconnect.casestat.body.IPSAufenthaltSchweregradL3&quot;]">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Aufenthalt in einer Intensivstation: Die Angaben zum Schwergrad der akuten Erkrankung für MSK müssen mit dem Template 'alisconnect.casestat.body.IPSAufenthaltSchweregradL3' deklariert werden</xhtml:p>
		</assert>
		<assert id="entity-alisconnect-casestat-0708" test="cda:entryRelationship/cda:observation/cda:templateId[@root=&quot;2.16.756.5.30.PlaceholderCaseStatV1&quot; and @extension=&quot;alisconnect.casestat.body.IPSAufenthaltNEMSL3&quot;]">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Aufenthalt in einer Intensivstation: Die Angaben zu NEMS für MSK müssen mit dem Template 'alisconnect.casestat.body.IPSAufenthaltNEMSL3' deklariert werden</xhtml:p>
		</assert>
	</rule>
</pattern>
<pattern>
	<!--
	********************************************************
	MD-Intensivstation: IPSAufenthaltBeatmungL3
	********************************************************
	-->
	<rule context="*[cda:templateId/@root=&quot;2.16.756.5.30.PlaceholderCaseStatV1&quot; and cda:templateId/@extension=&quot;alisconnect.casestat.body.IPSAufenthaltBeatmungL3&quot;]">
		<!-- Rule Documentation -->
		<xhtml:h3 lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">ALIS-Connect Case Statistics (casestat): CDA Body Level3 Template 'MD-Intensivstation künstliche Beatmung'</xhtml:h3>
		<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Die nachfolgenden Regeln beschreiben die Prüfungen zum CDA Body Entry 'MD-Intensivstation künstliche Beatmung'. Hinweis: Wo die Beatmungszeit nur in Form von Stunden und nicht als Zeitraum (von/bis) verfügbar ist, im 'low value' ein Datum mit Zeitpunkt 00:00 Uhr und im 'high value' die Anzahl Stunden als Differenz zum 'low value' angeben</xhtml:p>
		<!-- Deklaration als Encounter -->
		<assert id="entity-alisconnect-casestat-0801" test="self::cda:encounter">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Die künstliche Beatmung in einer Intensivstation für MD-Intensivmedizin muss als 'encounter' mittels entryRelationship zum Entry gemäss 'IPSAufenthaltL3' deklariert werden</xhtml:p>
		</assert>
		<!-- Fallnummer -->
		<assert id="entity-alisconnect-casestat-0802" test="cda:id">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Künstliche Beatmung in einer Intensivstation: Die Fallnummer muss als 'id' deklariert werden</xhtml:p>
		</assert>
		<!-- MSK Aufenthalt in einer Intensivstation: Beginn künstliche Beatmung -->
		<assert id="entity-alisconnect-casestat-0803" test="cda:effectiveTime/cda:low/@value">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Künstliche Beatmung in einer Intensivstation: Der Beginn einer künstlichen Beatmung muss deklariert werden (dient der Berechnung des Dauer der künstlichen Beatmung in der Codierabteilung)</xhtml:p>
		</assert>
		<!-- MSK Aufenthalt in einer Intensivstation: Ende künstliche Beatmung -->
		<assert id="entity-alisconnect-casestat-0804" test="cda:effectiveTime/cda:high/@value">
			<xhtml:p lang="de_ch" xmlns:xhtml="http://www.w3.org/1999/xhtml">Künstliche Beatmung in einer Intensivstation: Das Ende einer künstlichen Beatmung muss deklariert werden (dient der Berechnung des Dauer der künstlichen Beatmung in der Codierabteilung)</xhtml:p>
		</assert>
	</rule>
</pattern>
<pattern>
	<!--
	********************************************************
	MD-Intensivstation: IPSAufenthaltSchweregradL3
	********************************************************
	-->
	<rule context="*[cda:templateId/@root=&quot;2.16.756.5.30.PlaceholderCaseStatV1&quot; and cda:templateId/@extension=&quot;alisconnect.casestat.body.IPSAufenthaltSchweregradL3&quot;]">
		<!-- Rule Documentation -->
